<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="/editor/index.html" />
            </files>
        </defaultDocument>
        <directoryBrowse enabled="true" />
        <staticContent>
            <mimeMap fileExtension=".mtl" mimeType="text/plain" />
            <mimeMap fileExtension=".obj" mimeType="text/plain" />
            <mimeMap fileExtension=".dae" mimeType="model/vnd.collada+xml" />
        </staticContent>
    </system.webServer>
</configuration>
